{
  "compilerOptions": {
    "lib": [
      "es6",
      "dom"
    ],
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "dist/js",
    "noEmitOnError": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "jsx": "react"
  },
  "include": [
    "src"
  ]
}
